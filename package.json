{
  "name": "sagui",
  "version": "6.5.1",
  "description": "Front-end tooling in a single dependency",
  "preferGlobal": false,
  "bin": {
    "sagui": "./bin/sagui.js"
  },
  "main": "index.js",
  "files": [
    ".babelrc",
    "bin",
    "src",
    "lib",
    "template",
    ".eslintrc"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore *.spec.js",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint .",
    "test:integration": "mocha --timeout 0 --compilers js:babel-register --recursive --reporter spec --fgrep [integration] ./src",
    "test:unit": "mocha --timeout 0 --compilers js:babel-register --recursive --reporter spec --fgrep [integration] --invert ./src",
    "test:unit:watch": "npm run test:unit -- --watch",
    "prepublish": "not-in-install && npm prune && npm test && npm run build || in-install",
    "postinstall": "node ./bin/sagui.js install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pirelenito/sagui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pirelenito/sagui/issues"
  },
  "homepage": "https://github.com/pirelenito/sagui#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-register": "^6.7.2",
    "chai": "^3.3.0",
    "in-publish": "^2.0.0",
    "mocha": "^2.3.3",
    "tmp": "0.0.28"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-sagui": "^6.5.1",
    "chalk": "^1.1.1",
    "clean-webpack-plugin": "^0.1.9",
    "commander": "^2.8.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "~2.2.0",
    "eslint-config-sagui": "^1.0.1",
    "eslint-config-standard": "5.1.0",
    "eslint-config-standard-jsx": "1.1.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.7.2",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "lodash.uniq": "^4.3.0",
    "node-sass": "^3.3.3",
    "null-loader": "^0.1.1",
    "parent-module": "^0.1.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "postcss-modules-values": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-transform-hmr": "^1.0.4",
    "resolve-url-loader": "^1.4.4",
    "sass-loader": "^3.2.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "template-directory": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.7.3",
    "yaml-loader": "^0.2.0"
  },
  "peerDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-sagui": "^6.5.1",
    "cross-env": "^1.0.8",
    "eslint": "~2.2.0",
    "eslint-config-sagui": "^1.0.1",
    "eslint-config-standard": "5.1.0",
    "eslint-config-standard-jsx": "1.1.1",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.1"
  }
}
